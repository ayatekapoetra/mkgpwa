{
  "name": "femkgpwa",
  "version": "1.2.4",
  "description": "MKG Desktop Application - Mining Equipment Management System",
  "author": {
    "name": "Ayat Ekapoetra",
    "email": "ayatekapoetra@gmail.com",
    "url": "https://makkuragatama.id/"
  },
  "private": true,
  "license": "UNLICENSED",
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "rm -rf .next && next build",
    "build:electron": "cross-env ELECTRON_BUILD=true yarn build",
    "start": "next start -p 3005",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3005 electron .",
    "electron:dev": "concurrently \"yarn dev\" \"yarn electron\"",
    "electron:prod": "cross-env ELECTRON_START_URL= electron .",
    "electron:build": "npm run build:electron",
    "electron:build:start": "npm run build:electron && npm start",
    "electron:full": "npm run build:electron && concurrently \"npm start\" \"npm run electron:prod\"",
    "electron:pack": "npm run build:electron && electron-builder --dir",
    "electron:dist": "npm run build:electron && electron-builder",
    "electron:dist:mac": "npm run build:electron && electron-builder --mac",
    "electron:dist:mac:never": "npm run build:electron && npx --no-install electron-builder --mac --publish never",
    "electron:dist:win": "npm run build:electron && electron-builder --win",
    "electron:dist:win:never": "npm run build:electron && npx --no-install electron-builder --win --publish never",
    "electron:dist:linux": "npm run build:electron && electron-builder --linux",
    "electron:publish": "npm run build:electron && electron-builder --publish always",
    "electron:publish:mac": "npm run build:electron && electron-builder --mac --publish always",
    "electron:publish:win": "npm run build:electron && electron-builder --win --publish always",
    "build:static": "node build-electron.js",
    "lint": "next lint --quiet",
    "clean": "rm -rf .next out node_modules/.cache",
    "prettier": "prettier --write ./src"
  },
  "dependencies": {
    "@aws-sdk/client-textract": "^3.840.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/base": "^5.0.0-beta.28",
    "@mui/icons-material": "^5.15.1",
    "@mui/lab": "^5.0.0-alpha.157",
    "@mui/material": "^5.15.1",
    "@mui/system": "^5.15.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-spring/web": "^10.0.1",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-table": "^8.21.3",
    "apexcharts": "^5.3.3",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.22.0",
    "browser-image-compression": "^2.0.2",
    "chance": "^1.1.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.16",
    "history": "^5.3.0",
    "iconsax-react": "^0.0.8",
    "idb": "^8.0.3",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.439.0",
    "match-sorter": "^8.0.2",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "node-tesseract-ocr": "^2.2.1",
    "notistack": "^3.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-csv": "^2.2.2",
    "react-device-detect": "^2.2.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-input-mask": "^2.0.4",
    "react-intersection-observer": "^9.16.0",
    "react-intl": "^6.5.5",
    "react-slick": "^0.29.0",
    "react-spring": "^9.7.3",
    "react-table": "^7.8.0",
    "react-table-sticky": "^1.1.3",
    "react-timer-hook": "^3.0.7",
    "react-to-print": "^2.14.15",
    "react-window": "^1.8.10",
    "react18-input-otp": "^1.1.4",
    "sharp": "^0.34.4",
    "simplebar-react": "^3.2.4",
    "slick-carousel": "^1.8.1",
    "styled-jsx": "^5.1.2",
    "stylis-plugin-rtl": "^2.1.1",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "4.1.4",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "xlsx": "^0.18.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@date-io/date-fns": "^2.17.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^31.3.0",
    "electron-builder": "^24.13.3",
    "electron-icon-maker": "^0.0.5",
    "encoding": "^0.1.13",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "immutable": "^4.3.4",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.4"
  },
  "build": {
    "appId": "com.mkg.desktop",
    "productName": "MKG-Desktop",
    "directories": {
      "output": "dist"
    },
    "publish": {
      "provider": "github",
      "owner": "ayatekapoetra",
      "repo": "mkgpwa"
    },
    "files": [
      "electron/**/*",
      "out/**/*",
      "public/**/*",
      ".next/**/*",
      "node_modules/**/*",
      "docs/**/*",
      "README.md",
      "CHANGELOG.md"
    ],
    "extraResources": [
      {
        "from": "public",
        "to": "public"
      },
      {
        "from": "docs",
        "to": "docs"
      }
    ],
    "mac": {
      "category": "public.app-category.business",
      "target": [
        {
          "target": "dmg",
          "arch": "arm64"
        },
        {
          "target": "zip",
          "arch": "arm64"
        }
      ],
      "icon": "public/icons/logo.png",
      "extendInfo": {
        "CFBundleDisplayName": "MKG Desktop",
        "CFBundleIdentifier": "com.mkg.desktop",
        "CFBundleVersion": "1.0.0",
        "CFBundleShortVersionString": "1.0.0",
        "LSUIElement": false,
        "NSHighResolutionCapable": true
      },
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icons/logo.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
